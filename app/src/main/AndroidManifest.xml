<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shellybekhor.dexi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dexi_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dexi_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameSylsActivity" ></activity>
        <activity android:name=".GameWordActivity"></activity>
        <activity android:name=".WinnerActivity"></activity>

    </application>

</manifest>